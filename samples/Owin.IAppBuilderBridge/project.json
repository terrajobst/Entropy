{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Owin": "1.0.2",
    "Microsoft.AspNetCore.DataProtection": "1.0.2",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.2",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.3",
    "Microsoft.Extensions.DependencyInjection": "1.0.2",
    "Microsoft.Owin": "3.0.0"
  },
  "frameworks": {
    "net451": {}
  },
  "publishOptions": {
    "include": [
      "web.config"
    ]
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview4-final"
  },
